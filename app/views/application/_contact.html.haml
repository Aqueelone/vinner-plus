    #s-message.slide.fp-slide.fp-table{:style => "width: 50%;"}
      .fp-tableCell.fp-content{:style => "height: 609px;"}
        %section.content
          %header.p-title
            %h3
              Напишите нам!
              %i.ion.ion-email
            %ul.buttons
              %li.show-for-medium-up
                %a{:href => "#con", :title => "Контакты"}
                  %i.ion.ion-android-information
              %li
                %a{:href => "#con/1", :title => "Написать нам"}
                  %i.ion.ion-email
              %li
                %a{:href => "#con/2", :title => "Онлайн ЧАТ"}
                  %i.ion.ion-chatboxes
          /
          /      <li>
          /      <a title="Message" href="#contact/message"><i class="ion ion-email"></i></a>
          /      </li>
          / Begin Of Page SubSction
          .page-block.c-right.v-zoomIn
            .wrapper
              %div
                %form.message.form.send_message_form{:action => "ajaxserver/serverfile.php", :method => "get"}
                  .fields.clearfix
                    .input
                      %label{:for => "mes-name"} Имя
                      %input#mes-name{:name => "name", :placeholder => "Ваше имя", :required => "", :type => "text"}/
                    .buttons
                      %button#submit-message.button.email_b{:name => "submit_message"} Ok
                  .fields.clearfix
                    .input
                      %label{:for => "mes-email"} Email
                      %input#mes-email{:name => "email", :placeholder => "Ваш Email", :required => "", :type => "email"}/
                  .fields.clearfix.no-border
                    %label{:for => "mes-text"} Сообщение
                    %textarea#mes-text{:name => "message", :placeholder => "Ваше сообщение напишите здесь ...", :required => ""}
                    %div
                      %p.message-ok.invisible Ваше сообщение отправлено, СПАСИБО!.
    / End Of Page SubSction
